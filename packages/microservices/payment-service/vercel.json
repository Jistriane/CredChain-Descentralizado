{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3004",
    "SERVICE_NAME": "payment-service",
    "CORS_ORIGINS": "https://credchain-mainnet.vercel.app,https://credchain.vercel.app",
    "RATE_LIMIT_WINDOW_MS": "900000",
    "RATE_LIMIT_MAX_REQUESTS": "100",
    "JWT_SECRET": "credchain_payment_jwt_secret_production_2024",
    "REDIS_URL": "redis://redis.credchain.io:6379",
    "DATABASE_URL": "postgresql://credchain:password@postgres.credchain.io:5432/credchain",
    "STRIPE_SECRET_KEY": "sk_live_stripe_secret_key",
    "STRIPE_PUBLISHABLE_KEY": "pk_live_stripe_publishable_key",
    "PAYPAL_CLIENT_ID": "paypal_client_id",
    "PAYPAL_CLIENT_SECRET": "paypal_client_secret",
    "PAYPAL_MODE": "live",
    "POLKADOT_RPC_URL": "https://rpc.polkadot.io",
    "BLOCKCHAIN_ENABLED": "true",
    "CRYPTO_PAYMENTS_ENABLED": "true",
    "FIAT_PAYMENTS_ENABLED": "true",
    "LOG_LEVEL": "info",
    "LOG_FORMAT": "json"
  },
  "regions": ["iad1"],
  "framework": null
}
